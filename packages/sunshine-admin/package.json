{
  "name": "@sunshine/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 pnpm vite build",
    "build:analyze": "cross-env NODE_OPTIONS=--max-old-space-size=8192 pnpm vite build --mode analyze",
    "build:docker": "vite build --mode docker",
    "build:no-cache": "pnpm store prune && npm run build",
    "build:test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 pnpm vite build --mode test",
    "dev": "vite",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint \"**/*.{css,less,scss}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@ant-design/pro-layout": "^7.19.7",
    "@logicflow/core": "^1.2.26",
    "@logicflow/extension": "^1.2.26",
    "@purge-icons/generated": "^0.10.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.9.15",
    "@zxcvbn-ts/core": "^3.0.4",
    "ahooks": "^3.8.0",
    "antd": "5.16.5",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "eslint": "^8.56.0",
    "exceljs": "^4.4.0",
    "i18next": "^23.11.3",
    "i18next-browser-languagedetector": "^7.2.1",
    "lint-staged": "^15.2.2",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.3",
    "qs": "^6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.1",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.23.0",
    "react-transition-group": "^4.4.5",
    "stylelint": "^16.4.0",
    "turbo": "^1.13.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.59.4",
    "vite-plugin-mock": "2.9.8",
    "zustand": "^4.5.2",
    "sortablejs": "^1.15.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-transition-group": "^4.4.10",
    "cross-env": "^7.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-perfect-console-plugin": "^1.0.2",
    "@types/sortablejs": "^1.15.8"
  }
}